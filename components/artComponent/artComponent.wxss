.art {
    margin: 50rpx 0rpx 50rpx 50rpx;
    display: inline-block;
    position: relative;
}
.art-one {
    width: 400rpx;
}

.art-one-right {
    margin-left: 250rpx;
}

.left-art-txt {
    position: absolute;
    display: inline-block;
    width: 120rpx;
    bottom: 220rpx;
    padding: 0 30rpx;
    margin: 0 30rpx;
    text-align: center;
}
.left-art-txt-right{
    left: 0;
}
.top-left {
    position: absolute;
    top: 10rpx;
    left: 10rpx;
    height: 150rpx;
    width: 113rpx;
    border-left: 2rpx solid #4a4131;
    border-top: 2rpx solid #4a4131;
}

.hrt-right {
    height: 115rpx;
    width: 2rpx;
    position: absolute;
    background: #4a4131;
    top: 0rpx;
    right: 0rpx;
}

.hrt-bottom {
    width: 155rpx;
    height: 60rpx;
    border: 2rpx solid #4a4131;
    border-top: none;
    position: absolute;
    top: 200rpx;
    right: 10rpx;
}

.list1 {
    width: 100rpx;
}

.left-art-txt-bottom {
    display: inline-block;
    position: absolute;
    font-size: 30rpx;
    width: 210rpx;
    margin: 0rpx 10rpx 30rpx 40rpx;
    bottom: 10rpx;
    text-align: left;
}

.left-art-txt-bottom text {
    margin-left: 10rpx;
}

.info-top-left {
    width: 30rpx;
    height: 30rpx;
    margin-left: 0rpx;
    border-top: 2rpx solid #4a4131;
    border-left: 2rpx solid #4a4131;
}

.infof-right-bottom {
    position: absolute;
    width: 30rpx;
    height: 30rpx;
    right: 20rpx;
    bottom: -10rpx;
    border-right: 2rpx solid #4a4131;
    border-bottom: 2rpx solid #4a4131;
}

.fs20 {
    font-size: 20rpx;
}

.fs42 {
    font-size: 42rpx;
}

.text-block {
    margin: auto;
    display: block;
    width: 90rpx;
}

.txt-width {
    font-size: 24rpx;
}

.art-img {
    position: absolute;
}

.art-img1 {
    left: 50rpx;
    bottom: -100rpx;
    width: 350rpx;
}

.art-img2 {
    bottom: -100rpx;
    right: -50rpx;
    width: 460rpx;
}

.art-img3 {
    bottom: -100rpx;
    width: 380rpx;
    left: 20rpx;
}

.art-img4 {
    bottom: -100rpx;
    width: 410rpx;
    right: -20rpx;
}
.art-img5{
    bottom: -100rpx;
    width: 440rpx;
    left: -20rpx;
}
.art-img6 {
    width: 450rpx;
    right: -20rpx;
    bottom: 50rpx;
}
.txt-border-pad {
    padding-top: 30rpx;
    padding-bottom: 30rpx;
}
.txt-border {
    background-image: url("data:image/png;base64,");
    background-repeat: no-repeat;
    background-size: 180rpx 100%;
    width: 180rpx;
    height: 100%;
    margin-left: -20rpx;
    margin-bottom: 20rpx;
}